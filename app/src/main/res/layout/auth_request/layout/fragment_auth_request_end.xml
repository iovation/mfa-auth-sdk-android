<?xml version="1.0" encoding="utf-8"?>
<com.launchkey.android.authenticator.sdk.ui.internal.view.DynamicThemeAuthContentBgFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/ioa_theme_layouts_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="#fff">

    <ImageView
        android:id="@+id/auth_end_image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:adjustViewBounds="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:maxHeight="144dp"
        android:scaleType="fitCenter"
        android:screenReaderFocusable="true"
        app:tint="#444"
        tools:src="@drawable/ic_result_denied"
        tools:tint="#DB416A" />
</com.launchkey.android.authenticator.sdk.ui.internal.view.DynamicThemeAuthContentBgFrameLayout>