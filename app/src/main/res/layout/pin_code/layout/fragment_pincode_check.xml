<?xml version="1.0" encoding="utf-8"?>
<com.launchkey.android.authenticator.sdk.ui.internal.view.DynamicThemeBackgroundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/ioa_theme_layouts_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/authenticatorColorBackground"
    android:orientation="vertical">

    <include
        android:id="@+id/pincode_check_header"
        layout="@layout/toinclude_method_addcheck_header"
        tools:layout="@layout/toinclude_method_addcheck_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeView
        android:id="@+id/pincode_check_pinpad"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</com.launchkey.android.authenticator.sdk.ui.internal.view.DynamicThemeBackgroundLinearLayout>