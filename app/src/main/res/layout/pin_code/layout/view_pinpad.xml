<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="1"
        app:labelText=""/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="2"
        app:labelText="ABC"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="3"
        app:labelText="DEF"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="4"
        app:labelText="GHI"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="5"
        app:labelText="JKL"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="6"
        app:labelText="MNO"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_7"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="7"
        app:labelText="PQRS"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_8"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="8"
        app:labelText="TUV"/>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_9"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="9"
        app:labelText="WXYZ"/>

    <FrameLayout
        android:id="@+id/pp_imagebutton_delete_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@string/ioa_acc_pincode_delete"
        >

        <com.launchkey.android.authenticator.sdk.ui.internal.view.AutoInvalidatingImageButton
            android:id="@+id/pp_image_delete"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_delete_black_24dp"
            android:scaleType="fitCenter"
            android:background="@null"
            android:layout_gravity="center"
            />
    </FrameLayout>

    <com.launchkey.android.authenticator.sdk.ui.internal.auth_method.pin_code.PinCodeKey
        android:id="@+id/pp_button_0"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:numberText="0"
        app:labelText=""
        app:numberOnly="true"
        />

    <FrameLayout
        android:id="@+id/pp_imagebutton_check_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"
        android:contentDescription="@string/ioa_acc_pincode_submit"
        >

        <com.launchkey.android.authenticator.sdk.ui.internal.view.AutoInvalidatingImageButton
            android:id="@+id/pp_imagebutton_check"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_done_black_24dp"
            android:scaleType="fitCenter"
            android:background="@null"
            android:layout_gravity="center"
            />
    </FrameLayout>
</merge>