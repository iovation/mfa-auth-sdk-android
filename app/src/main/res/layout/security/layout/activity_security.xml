<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/ioa_theme_layouts_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/security_toolbar"
        layout="@layout/toinclude_toolbar"/>

    <androidx.fragment.app.FragmentContainerView
        android:name="com.launchkey.android.authenticator.sdk.ui.SecurityFragment"
        android:id="@+id/security_securityfragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_below="@id/security_toolbar"
        android:layout_alignParentBottom="true"/>
</RelativeLayout>